{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "ES2022",
    "target": "es2022",
    "types": ["vite/client"],
    "baseUrl": ".",
    "paths": {
      "@layouts/*": ["src/components/layouts/*.astro"],
      "@components/*": ["src/components/*.astro"],
      "@data/*": ["src/data/*.ts"],
      "@assets/*": ["src/assets/*"],
      "@/*": ["src/*.ts"]
    }
  }
}
